*color2:  #4F378F
*.color3: #0046D0
*color3:  #0046D0
*.color4: #5E5F9B
*color4:  #5E5F9B
*.color5: #97498B
*color5:  #97498B
*.color6: #749FA8
*color6:  #749FA8
*.color7: #bfcdc8
*color7:  #bfcdc8
*.color8: #858f8c
*color8:  #858f8c
*.color9: #C1A050
*color9:  #C1A050
*.color10: #4F378F
*color10:  #4F378F
*.color11: #404592
*color11:  #404592
*.color12: #5E5F9B
*color12:  #5E5F9B
*.color13: #97498B
*color13:  #97498B
*.color14: #749FA8
*color14:  #749FA8
*.color15: #bfcdc8
*color15:  #bfcdc8

! Black color that will not be affected by bold highlighting.
*.color66: #120d1b
*color66:  #120d1b

! Xclock colors.
XClock*foreground: #bfcdc8
XClock*background: #120d1b
XClock*majorColor:  rgba:bf/cd/c8/ff
XClock*minorColor:  rgba:bf/cd/c8/ff
XClock*hourColor:   rgba:bf/cd/c8/ff
XClock*minuteColor: rgba:bf/cd/c8/ff
XClock*secondColor: rgba:bf/cd/c8/ff

! Set depth to make transparency work.
URxvt*depth: 32
URxvt.transparent: true
URxvt.shading: 10

URxvt.font: xft:TerminessTTF Nerd Font:size=11
URxvt.scrollBar: false
